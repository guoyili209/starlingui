<?xml version="1.0" encoding="UTF-8"?>
<comps>
	<Window id="window" title="新版本检测" hasCloseButton="true" draggable="true" width="230" height="90" event="close:onClose">
		<VBox x="12" y="8">
			<HBox>
				<Label text="有新版本可以使用!是否更新?"/>
			</HBox>
			<HBox>
				<PushButton id="yesBtn" label="是" event="click:onYes"/>
				<PushButton id="noBtn" label="否" event="click:onNo"/>
			</HBox>
		</VBox>
	</Window>
</comps>
