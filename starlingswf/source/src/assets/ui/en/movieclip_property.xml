<?xml version="1.0" encoding="UTF-8"?>
<comps>
	<Panel width="160" height="134">
		<VBox x="10" y="10" spacing="6">
			<Label text="MovieClip Property"/>
			<HBox>
				<Label text="Total Frames"/>
				<Label id="totalFrames" />
			</HBox>
			<HBox>
				<Label text="Label"/>
				<ComboBox id="labelsComboBox" width="100" enabled="false" event="select:onSelectLabels" />
			</HBox>
			<HBox>
				<Label text="Loop"/>
				<CheckBox id="isLoop" selected="true" event="click:onChangeLoop" />
			</HBox>
			<PushButton label="Play" event="click:onPlay" />
		</VBox>
	</Panel>
</comps>
